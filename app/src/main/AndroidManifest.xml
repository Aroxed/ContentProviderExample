<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.kpi.spscs.mycontentprovider">

    <permission android:name="ua.kpi.spscs.mycontentprovider.provider.READWRITE"/>
    <uses-permission android:name="ua.kpi.spscs.mycontentprovider.provider.READWRITE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyContentProvider">
        <provider
            android:name=".MyContentProvider"
            android:authorities="com.random"
            android:enabled="true"
            android:exported="true"
        />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>